<Window
    x:Class="Sample.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:iri="https://irihi.tech/shared"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Sample"
    mc:Ignorable="d">
    <StackPanel HorizontalAlignment="Left">
        <iri:PureRectangle Width="30" Height="40" Background="Red" HorizontalAlignment="Left"/>
        <iri:PureCircle Diameter="50" Background="Blue"></iri:PureCircle>
        <Panel>
            <Button>Hell World</Button>
            <iri:PureRing Diameter="50" InnerDiameter="25" Background="Green"></iri:PureRing>
        </Panel>
    </StackPanel>
</Window>
